<?xml version="1.0" encoding="UTF-8"?>

<configuration>
    <input>
        <net-file value="complex_network_proper.net.xml"/>
        <route-files value="simple_routes_fixed.rou.xml"/>
    </input>

    <time>
        <begin value="0"/>
        <end value="3600"/>
        <step-length value="1"/>
    </time>

    <processing>
        <collision.action value="warn"/>
        <max-depart-delay value="300"/>
    </processing>

    <gui_only>
        <start value="true"/>
        <quit-on-end value="false"/>
    </gui_only>

    <report>
        <verbose value="false"/>
        <no-step-log value="true"/>
    </report>

</configuration>